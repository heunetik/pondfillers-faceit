{
  "manifest_version": 2,

  "name": "pondfillers TeamSpeak 3 Plugin",
  "short_name": "pondfillersTS",
  "description": "Displays current status of the server & the people online!",
  "version": "1.0.2",

  "icons": 
  {
    "128": "icon_128.png",
    "64": "icon_64.png",
    "16": "icon_16.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.htm",
    "default_title": "http://steamcommunity.com/groups/pondfillers"
  },
  "permissions": 
  [
    "tabs",
    "http://ts.pondfillers.net"
  ]
}