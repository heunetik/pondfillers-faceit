{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "pondfillers' FACEIT STAT CHECKER",
  "short_name": "pondfillers Faceit",
  "description": "Faceit CSGO stats checker",
  "version": "4.3.3",
  "homepage_url": "https://chrome.google.com/webstore/detail/pondfillers-faceit-stat-c/gbellpiiocaokppipflckdfmnmpmcmpc",
  "icons": 
  {
    "128": "icons/icon_128.png",
    "64": "icons/icon_64.png",
    "16": "icons/icon_16.png"
  },
  "browser_action": 
  {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.htm",
    "default_title": "pondfillers' FACEIT STAT CHECKER"
  },
  "permissions": [
          "storage"
        ],
  "background":
  {
    "scripts": 
    [
      "js/faceit.js"
    ]
  }
}
